{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react-native",
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "$/*": ["./stuff/*"],
    },
  },
  "include": [
    "plugins",
    "stuff",
    "lang",
    "node_modules/vendetta-types",
    "global.d.ts",
  ],
  "exclude": ["archive", "dist", "scripts"],
}
